[HKEY_CLASSES_ROOT\Folder\shell\GETEVIDENCE\command]
@="cmd.exe /c \"xcopy /E /I D:\\xxx\\LOG\\DEBUG \"%1\"\\DEBUG && move C:\\mail\\EVIDENCE\\*.jpg \"%1\" \""

